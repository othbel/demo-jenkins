pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Build stage, branch: feature-01"'
            }
            
        }
        stage('Test') {
            steps {
                sh 'echo "Test stage, branch: feature-01"'    
            }
            
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploy stage, branch: feature-01"'    
            }
            
        }
    }
}